'use strict';

/**
 * Configuration file to be used for routes
 */

const routeApiPrefix = 'api';
const routeVersions = {
  v1: 'v1',
};
const routes: any = []; // Application routes

export default {
  setRoutes: (app: any) => {
    routes.forEach((route: any) => {
      app.use(`/${routeApiPrefix}/${route.version}/${route.route}`, require(route.path));
    });

    return app;
  },
};
